import "hashes/sha256/512bitPacked" as sha256packed;

def main(
    private field _balance, 
    private field _hash_secret_key, 
    #private field c,
    #private field d,
    field h1,field h2
    ) -> bool {
    field[2] h = sha256packed([_balance, _hash_secret_key]);
    #field[2] h = sha256packed([a,b,c,d]);
    return (h[0]==h1)&&(h[1]==h2);
}